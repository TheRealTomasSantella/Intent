<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.mrsunnybunny.activity2">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".married"
            android:label="@string/title_activity_married"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".single"
            android:label="@string/title_activity_single"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".education"
            android:label="@string/title_activity_education"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".languages"
            android:label="@string/title_activity_languages"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".Finish"
            android:label="@string/title_activity_finish"
            android:theme="@style/AppTheme.NoActionBar"></activity>
    </application>

</manifest>
